    <script src="~/Resources/js/angular-modules/reportes/arbol-transacciones.js"></script>
    <script src="~/Resources/js/full-url.js" type="text/javascript"></script>
    <script src="~/Resources/js/validator.min.js" type="text/javascript"></script>
    <script src="~/Resources/js/sidebar-toggle.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/tabs.js" type="text/javascript"></script>
    <script src="~/Resources/js/ng-bootstrap.treeview/dist/abn_tree_directive.js" type="text/javascript"></script>
    <script src="~/Resources/js/ng-bootstrap.treeview/dist/abn_tree_btn_directive.js" type="text/javascript"></script>
    <script src="~/Resources/js/ng-bootstrap.treeview/dist/abn_tree_directive_cantidad.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/treeview.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/routing.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/usuario.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/grupo.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/programa.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/acceso.js" type="text/javascript"></script>
