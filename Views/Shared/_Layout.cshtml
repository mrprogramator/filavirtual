    <script src="~/Resources/js/angular-modules/ticketera.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/confticketera.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/agente.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/atencion.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/atencion-psql.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/parametro.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/punto.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/mesatipos.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/maquina.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/exportador-atenciones.js" type="text/javascript"></script>
    <script src="~/Resources/js/angular-modules/monitoreo-mesas.js" type="text/javascript"></script>
    <script src="~/Resources/metrics-graphics/d3.js" charset="utf-8"></script>
    <script src="~/Resources/js/angular-modules/reporte.js" type="text/javascript"></script>
    <script src="~/Resources/metrics-graphics/metricsgraphics.js" type="text/javascript"></script>
    <script src="~/Scripts/kendo/ReportViewer-8.0.14.507.js"></script>
    
</body>
</html>
